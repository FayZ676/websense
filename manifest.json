{
    "name": "Websense",
    "version": "0.1.0",
    "description": "Websense description",
    "permissions": [
        "tabs",
        "activeTab",
        "downloads",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "manifest_version": 3
}